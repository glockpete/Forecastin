# Vite App Configuration
NODE_ENV=development

# API Configuration
VITE_API_URL=http://localhost:9000
VITE_API_PORT=9000
VITE_API_URL_OVERRIDE=

# WebSocket Configuration
VITE_WS_URL=ws://localhost:9000/ws
VITE_WS_PORT=9000
VITE_WS_URL_OVERRIDE=

# Feature Flags (Local Development)
# These can be overridden locally for testing
# Format: VITE_FF_<FLAG_NAME>=true/false
VITE_FF_GEO_LAYERS_ENABLED=true
VITE_FF_GEO_CLUSTERING_ENABLED=true
VITE_FF_GEO_POINT_LAYER_ACTIVE=true
VITE_FF_GEO_WEBSOCKET_LAYERS_ENABLED=true

# Development Tools
VITE_ENABLE_DEVTOOLS=true

# Backwards compatibility (deprecated - use VITE_ prefix instead)
# These are still read by src/config/env.ts for transition period
REACT_APP_API_URL=http://localhost:9000
REACT_APP_WS_URL=ws://localhost:9000/ws
