# React App Configuration
NODE_ENV=development

# API Configuration
REACT_APP_API_URL=http://localhost:9000
REACT_APP_API_PORT=9000
REACT_APP_API_URL_OVERRIDE=

# WebSocket Configuration
REACT_APP_WS_URL=ws://localhost:9000/ws
REACT_APP_WS_PORT=9000
REACT_APP_WS_URL_OVERRIDE=

# Vite Alternative Configuration (for Vite builds)
VITE_API_URL=http://localhost:9000
VITE_API_PORT=9000
VITE_API_URL_OVERRIDE=
VITE_WS_URL=ws://localhost:9000/ws
VITE_WS_PORT=9000
VITE_WS_URL_OVERRIDE=

# Feature Flags (Local Development)
# These can be overridden locally for testing
# Format: REACT_APP_FF_<FLAG_NAME>=true/false
REACT_APP_FF_GEO_LAYERS_ENABLED=true
REACT_APP_FF_GEO_CLUSTERING_ENABLED=true
REACT_APP_FF_GEO_POINT_LAYER_ACTIVE=true
REACT_APP_FF_GEO_WEBSOCKET_LAYERS_ENABLED=true

# Development Tools
REACT_APP_ENABLE_DEVTOOLS=true
